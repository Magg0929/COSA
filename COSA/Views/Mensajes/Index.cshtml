@model IEnumerable<COSA.Models.Mensaje>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPersona.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Mensaje1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Chat.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Costos.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Telefonos.Numero)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Mensaje1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Chat.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Costos.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Telefonos.Numero)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
